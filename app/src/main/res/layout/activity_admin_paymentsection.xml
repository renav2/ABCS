<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin_paymentsection">

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="378dp"
        android:layout_height="0dp"
        android:layout_marginTop="271dp"
        android:layout_marginBottom="67dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/button33"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.429">

        <Button
            android:id="@+id/admin_examrecord"
            android:layout_width="90dp"
            android:layout_height="85dp"
            android:layout_marginLeft="20dp"
            android:text="Exam payment record " />


        <Button
            android:id="@+id/admin_hostel_record"
            android:layout_width="90dp"
            android:layout_height="85dp"
            android:layout_marginLeft="30dp"
            android:text="Hostal payment record " />


        <Button
            android:id="@+id/Admin_clg_record"
            android:layout_width="90dp"
            android:layout_height="85dp"
            android:layout_marginLeft="30dp"
            android:text="College payment record " />


    </LinearLayout>

    <Button
        android:id="@+id/button33"
        android:layout_width="112dp"
        android:layout_height="0dp"
        android:layout_marginStart="66dp"
        android:layout_marginBottom="198dp"
        android:text="supportpay"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6" />

</androidx.constraintlayout.widget.ConstraintLayout>