<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin_paymentsection">

    <Button
        android:id="@+id/Admin_clg_record"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="294dp"
        android:layout_marginEnd="138dp"
        android:text="College payment record "
        app:layout_constraintBottom_toTopOf="@+id/admin_hostel_record"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/admin_hostel_record"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="145dp"
        android:layout_marginBottom="7dp"
        android:text="Hostal payment record "
        app:layout_constraintBottom_toTopOf="@+id/admin_examrecord"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/Admin_clg_record"
        app:layout_constraintTop_toBottomOf="@+id/Admin_clg_record" />

    <Button
        android:id="@+id/admin_examrecord"
        android:layout_width="243dp"
        android:layout_height="54dp"
        android:layout_marginEnd="143dp"
        android:layout_marginBottom="277dp"
        android:text="Exam payment record "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/admin_hostel_record"
        app:layout_constraintTop_toBottomOf="@+id/admin_hostel_record" />

</androidx.constraintlayout.widget.ConstraintLayout>